{
  "name": "react-starter",
  "version": "1.2.1",
  "description": "A React JS starter project",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production webpack --config webpack.config.production.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build-admin": "rm -rf dist && NODE_ENV=production webpack --config webpack.config.production.admin.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build-development": "rm -rf dist && NODE_ENV=development webpack --config webpack.config.production.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build-stage": "rm -rf dist && NODE_ENV=staging webpack --config webpack.config.production.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build-stage-admin": "rm -rf dist && NODE_ENV=staging webpack --config webpack.config.production.admin.js --color -p --progress --hide-modules --display-optimization-bailout",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.development.js",
    "dev-admin": "NODE_ENV=development webpack-dev-server --config webpack.config.development.admin.js",
    "install-fonts": "goofoffline 'https://fonts.googleapis.com/css?family=Lato&display=swap' outDir=src/assets/fonts",
    "init": "yarn install && yarn run install-fonts",
    "publish": "scotty --spa --source ./dist --bucket app.springboard.com --region us-west-2",
    "publish-admin": "scotty --spa --source ./dist --bucket admin.springboard.com --region us-west-2",
    "publish-beta": "scotty --spa --source ./dist --bucket beta.springboard.com --region us-west-2",
    "publish-development": "scotty --spa --source ./dist --bucket staging.springboard.com",
    "publish-stage": "scotty --spa --source ./dist --bucket staging.springboard.com",
    "start-server": "http-server ./dist",
    "test": "test"
  },
  "author": "Athman Gude <athmangude@gmail.com>",
  "license": "ISC",
